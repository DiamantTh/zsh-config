
# alias
alias ls='ls $LS_OPTIONS -hF'
alias ll='ls $LS_OPTIONS -lAhF'
alias wine32='WINEARCH=win32 WINEPREFIX=~/.wine32 /usr/bin/wine'
alias wine64='WINEARCH=win64 WINEPREFIX=~/.wine64 /usr/bin/wine64'
alias winetricks32='WINEARCH=win32 WINEPREFIX=~/.wine32 winetricks'
alias winetricks64='WINEARCH=win64 WINEPREFIX=~/.wine64 winetricks'
alias winecfg32='WINEARCH=win32 WINEPREFIX=~/.wine32 winecfg'
alias winecfg64='WINEARCH=win64 WINEPREFIX=~/.wine64 winecfg'
alias mono32='WINEARCH=win64 WINEPREFIX=~/.wine64 mono'
alias mono64='WINEARCH=win64 WINEPREFIX=~/.wine64 mono'
alias psx='ps aux'

alias weechat-curses='screen -S weechat -xR weechat-curses'
alias weechat='screen -S weechat -xR weechat-curses'
alias chat='screen -S weechat -xR weechat-curses'

